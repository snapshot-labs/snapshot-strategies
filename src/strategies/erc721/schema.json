{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "NFT symbol",
          "examples": ["e.g. DOODLE"]
        },
        "address": {
          "type": "string",
          "title": "NFT contract address",
          "examples": ["e.g. 0x1f9840a85d5af5bf1d1762f925bdaddc4201f984"]
        }
      },
      "required": ["symbol", "address"],
      "additionalProperties": false
    }
  }
}
