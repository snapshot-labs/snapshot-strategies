{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "gloves": {
          "type": "object",
          "title": "gloves",
          "examples": ["e.g. {\"0x06f68F90F95826Ac3f9806B1Cd66D3CaFe13a752\": 2}"]
        },
        "weightClassAddress": {
          "type": "string",
          "title": "weightClassAddress",
          "examples": ["e.g. 0xB876baF8F69cD35fb96A17a599b070FBdD18A6a1"],
          "pattern": "^0x[a-fA-F0-9]{40}$",
          "minLength": 42,
          "maxLength": 42
        },
        "weightClassIds": {
          "type": "object",
          "title": "weightClassIds",
          "examples": ["e.g. {\"602\": 1}"]
        },
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. FC"],
          "maxLength": 16
        }
      },
      "required": ["gloves", "weightClassAddress", "weightClassIds"],
      "additionalProperties": false
    }
  }
}
