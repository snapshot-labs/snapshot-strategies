{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. METRO"],
          "maxLength": 16
        },
        "id": {
          "type": "string",
          "title": "Token ID",
          "examples": ["1"]
        },
        "weight": {
          "type": "number",
          "title": "Weight",
          "examples": ["e.g. 100"]
        }
      },
      "required": ["id"],
      "additionalProperties": false
    }
  }
}
