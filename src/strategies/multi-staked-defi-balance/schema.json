{
    "type": "object",
    "properties": {
      "symbol": {
        "type": "string"
      },
      "cumulativeMinimumStakedBalance": {
        "type": "string"
      },
      "strategies": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "object",
          "required": ["name", "network", "params"],
          "properties": {
            "name": {
              "type": "string"
            },
            "network": {
              "type": "string"
            },
            "params": {
              "type": "object",
              "properties": {
                "tokenContractAddress": {
                  "type": "string",
                  "pattern": "^0x[a-fA-F0-9]{40}$"
                },
                "symbol": {
                  "type": "string"
                },
                "decimals": {
                  "type": "integer"
                },
                "minStakedBalance": {
                  "type": "string"
                },
                "stakingPoolContractAddress": {
                  "type": "string",
                  "pattern": "^0x[a-fA-F0-9]{40}$"
                },
                "stakingType": {
                  "oneOf": [
                    {
                      "type": "string",
                      "enum": [
                        "standard"
                      ]
                    },
                    {
                      "type": "string",
                      "enum": [
                        "open"
                      ]
                    }
                  ]
                }
              },
              "required": [
                "tokenContractAddress",
                "symbol",
                "decimals",
                "minStakedBalance",
                "stakingPoolContractAddress",
                "stakingType"
              ]
            }
          },
          "additionalProperties": false
        }
      }
    },
    "required": ["strategies", "cumulativeMinimumStakedBalance"],
    "additionalProperties": false
  }
  