{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Path balance, staked and locked",
      "type": "object",
      "properties": {
        "tokenAddress": {
          "type": "string",
          "title": "Token address",
          "examples": ["e.g. 0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984"]
        },
        "stakingAddress": {
          "type": "string",
          "title": "Staking address",
          "examples": ["e.g. 0x1f9840a85d5aF5bf1D1762F925BDADdC4201F984"]
        },
        "lockedAddresses": {
          "type": "array",
          "title": "Locked addresses",
          "examples": ["e.g. [\"0x67f0260254FB3Cee97dA18077927888Ed72D1f17\", \"0xeE729DB66431e4401D63A38a2048d8CE0DF96eC3\", \"0x8923f2A0465287E2F8564F85a7f62ed34d947594\",\"0xfB05dc346f36eE88680e885CaC881983FAdEcBD4\"]"]
        },
        "methodABI": {
          "type" : "string",
          "title": "Method ABI for locked addresses",
          "examples": ["function getRemainingAmount(address _recipient) external view returns (uint256 amount)"]
        },
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. UNI"]
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": ["e.g. 18"]
        }
      },
      "required": ["tokenAddress", "stakingAddress", "lockedAddresses", "methodABI", "symbol", "address", "decimals"],
      "additionalProperties": false
    }
  }
}
