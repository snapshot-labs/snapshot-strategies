{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "stakeManagerAddress": {
          "type": "string",
          "title": "Stake Manager Contract Address",
          "pattern": "^0x[a-fA-F0-9]{40}$",
          "minLength": 42,
          "maxLength": 42
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": [
            18
          ]
        }
      },
      "required": [
        "stakeManagerAddress",
        "decimals"
      ],
      "additionalProperties": false
    }
  }
}