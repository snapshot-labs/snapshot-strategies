{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. DAI"],
          "maxLength": 16
        },
        "address": {
          "type": "string",
          "title": "Contract address",
          "examples": ["e.g. 0x97cb342cf2f6ecf48c1285fb8668f5a4237bf862"],
          "pattern": "^0x[a-fA-F0-9]{40}$",
          "minLength": 42,
          "maxLength": 42
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": ["e.g. 18"]
        },
        "isGlobal": {
          "type": "boolean",
          "title": "Is Global (All Possible Recipients)",
          "examples": ["e.g. true"]
        }
      },
      "required": ["address", "decimals"],
      "additionalProperties": false
    }
  }
}
