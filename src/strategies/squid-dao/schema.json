{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. SQDV"]
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": ["e.g. 4"]
        }
      },
      "required": ["symbol", "decimals"],
      "additionalProperties": false
    }
  }
}
