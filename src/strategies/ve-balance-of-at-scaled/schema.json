{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. aveQI"],
          "maxLength": 16
        },
        "address": {
          "type": "string",
          "title": "Contract address",
          "examples": ["e.g. 0x1bffabc6dfcafb4177046db6686e3f135e8bc732"]
        },
        "decimals": {
          "type": "number",
          "title": "Decimals",
          "examples": ["e.g. 18"]
        },
        "scaleValue": {
          "type": "number",
          "title": "Scale Value",
          "examples": ["e.g. 4"]
        }
      },
      "required": ["address", "decimals", "scaleValue"],
      "additionalProperties": false
    }
  }
}
