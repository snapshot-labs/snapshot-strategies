{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/Strategy",
  "definitions": {
    "Strategy": {
      "title": "Strategy",
      "type": "object",
      "properties": {
        "symbol": {
          "type": "string",
          "title": "Symbol",
          "examples": ["e.g. VOTES"],
          "maxLength": 16
        },
        "purposeIdentifier": {
          "type": "string",
          "title": "Purpose identifier (defaults to snapshot space ens)",
          "examples": ["e.g. YourDaoName"]
        },
        "power": {
          "type": "number",
          "title": "Voting power per verified person (defaults to 1)",
          "examples": ["e.g. 1000"]
        }
      },
      "required": [],
      "additionalProperties": false
    }
  }
}
